<h1 class="text-center mb-3 my-4">Uplaoded Images</h1>
	<div class="text-center mb-3 my-4 m-auto" >
		<% items.forEach(function(image) { %>
		<div>
			<div>
				<img style="width: 30%" src="data:image/<%=image.img.contentType%>;base64,
					<%=image.img.data.toString('base64')%>">
				<div>
					<h5><%= image.name %></h5>
					<h1><%= image.desc %></h1>
				</div>
			</div>
		</div>
		<% }) %>
	</div>
